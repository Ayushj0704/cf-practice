###problem 1 : https://codeforces.com/problemset/problem/59/A
#include<bits/stdc++.h>
 
using namespace std;
int main(){
    string s;
    cin>>s;
    int n =  (s.length())/2;
    int cnt=0;
    for(char c: s){
        if(isupper(c)){
            cnt++;
        }
    }
    if(cnt>n){
        transform(s.begin(),s.end(),s.begin(),::toupper);
        cout<<s<<endl;
    }
    else{
        transform(s.begin(),s.end(),s.begin(),::tolower);
        cout<<s<<endl;
    }
    return 0;
}


######problem 2 : https://codeforces.com/problemset/problem/469/A
#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int n;
    cin >> n;
 
    vector<int> checker(n + 1, 0);
 
    int p;
    cin >> p;
    for (int i = 0; i < p; i++) {
        int level;
        cin >> level;
        checker[level] = 1;
    }
 
    int q;
    cin >> q;
    for (int i = 0; i < q; i++) {
        int level;
        cin >> level;
        checker[level] = 1;
    }
 
    bool allPassed = true;
    for (int i = 1; i <= n; i++) {
        if (checker[i] == 0) {
            allPassed = false;
            break;
        }
    }
 
    if (allPassed) {
        cout << "I become the guy." << endl;
    } else {
        cout << "Oh, my keyboard!" << endl;
    }
 
    return 0;
}
